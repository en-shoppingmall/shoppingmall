{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      nameMessage: '',\n      idMessage: '',\n      emailMessage: '',\n      searchOption: '',\n      name: '',\n      optionName: \"이름\",\n      id: '',\n      email: '',\n      searchNameOrId: ''\n    };\n  },\n\n  methods: {\n    chageSelect(event) {\n      if (event.target.value == \"id\") {\n        this.optionName = \"이름\";\n      } else {\n        this.optionName = \"아이디\";\n      }\n\n      this.searchNameOrId = this.optionName;\n    },\n\n    submit() {\n      if (this.id == '' || this.email == '') {\n        alert(\"누락된 입력이 있습니다.\");\n      } else this.searchInformation();\n    },\n\n    async searchInformation() {\n      // 로그인 함수 \n      return await axios({\n        url: \"http://13.209.68.70:3000/customer/find/ID\",\n        //가상서버 주소\n        method: \"post\",\n        // post 명령어 \n        data: {\n          \"param\": [this.id, this.email]\n        }\n      }).then(res => {\n        // 데이터 준 다음 받은 응답\n        alert(\"아이디 : \" + JSON.stringify(res.data));\n      }).catch(e => {\n        alert(\"일치하는 계정이 없습니다.\");\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACPC,IAAI,GAAE;IACN,OAAM;MACFC,WAAU,EAAI,EADZ;MAEFC,SAAQ,EAAI,EAFV;MAGFC,YAAW,EAAI,EAHb;MAIFC,YAAW,EAAI,EAJb;MAKFC,IAAG,EAAI,EALL;MAMFC,UAAU,EAAC,IANT;MAOFC,EAAE,EAAC,EAPD;MAQFC,KAAK,EAAC,EARJ;MASFC,cAAc,EAAC;IATb,CAAN;EAWC,CAbM;;EAcPC,OAAO,EAAC;IACRC,WAAW,CAACC,KAAD,EAAO;MACd,IAAGA,KAAK,CAACC,MAAN,CAAaC,KAAb,IAAsB,IAAzB,EAA8B;QAC1B,KAAKR,UAAL,GAAkB,IAAlB;MACJ,CAFA,MAGI;QACA,KAAKA,UAAL,GAAkB,KAAlB;MACJ;;MACA,KAAKG,cAAL,GAAsB,KAAKH,UAA3B;IACH,CATO;;IAURS,MAAM,GAAE;MACJ,IAAG,KAAKR,EAAL,IAAW,EAAX,IAAiB,KAAKC,KAAL,IAAc,EAAlC,EACA;QACIQ,KAAK,CAAC,eAAD,CAAL;MACJ,CAHA,MAIK,KAAKC,iBAAL;IACR,CAhBO;;IAiBR,MAAMA,iBAAN,GAA0B;MAAE;MAC5B,OACA,MAAMlB,KAAK,CAAC;QACVmB,GAAG,EAAG,2CADI;QACyC;QACnDC,MAAM,EAAE,MAFE;QAEM;QAChBnB,IAAI,EAAE;UAAC,SAAS,CAAC,KAAKO,EAAN,EAAU,KAA