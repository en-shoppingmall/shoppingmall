 </div>\n                <button type = \"button\" class = \"enter\" @click=\"submit\">확인</button>\n            </form>\n        </div>\n        \n    </div>\n</template>\n\n\n\n<script>\nimport axios from 'axios'\nexport default { \n        data(){\n        return{\n            nameMessage : '',\n            idMessage : '',\n            emailMessage : '',\n            searchOption : '',\n            name : '',\n            optionName:\"이름\",\n            id:'',\n            email:'',\n            searchNameOrId:''   \n        }\n        },\n        methods:{    \n        chageSelect(event){\n            if(event.target.value == \"id\"){\n                this.optionName = \"이름\"\n            }\n            else{\n                this.optionName = \"아이디\";\n            }\n            this.searchNameOrId = this.optionName;\n        },\n        submit(){\n            if(this.id == '' || this.email == '')\n            {\n                alert(\"누락된 입력이 있습니다.\");\n            }\n            else this.searchInformation();\n        },\n        async searchInformation() { // 로그인 함수 \n        return (\n        await axios({\n          url:  \"http://13.209.68.70:3000/customer/find/ID\", //가상서버 주소\n          method: \"post\", // post 명령어 \n          data: {\"param\": [this.id, this.email]}\n        }).then((res) => { // 데이터 준 다음 받은 응답\n            alert(\"아이디 : \" + JSON.stringify(res.data))\n        })\n        .catch((e) => {\n          alert(\"일치하는 계정이 없습니다.\");\n        })\n      )\n    }\n    },  \n}\n</script>\n\n\n<style scoped>\n#user{\n    padding-right : 26%;\n}\n#description{\n    font-size : 13px;\n    opacity : 0.5;\n    font-weight : bold;\n    padding-top : 20px;\n}\n#id{\n    padding-right : 102px;\n}\n#email{\n    padding-right : 30%;\n}\n.user-select{\n    margin : 0;\n}\n.container{\n    padding-top : 150px;\n    font-size : 30px;\n}\n.user-area{\n    font-size : 14px;\n    width : 300px;\n    margin: 0 auto;\n    display : flex;\n}\n.search{\n    display : flex;\n    flex : 2;\n    text-align : left;\n    width : 40%;\n    padding : 3px;\n    margin : 3px;\n    opacity : 0.5;\n}\n.enter{\n    width : 300px;\n    margin: 0 auto;\n    margin-top : 2%;\n    padding : 0.5%;\n    font-size : 20px;\n    border : 1px;\n    color : white;\n    background : #333;\n}\n.enter:hover{\n    background : #ddd;\n    color : #333;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}