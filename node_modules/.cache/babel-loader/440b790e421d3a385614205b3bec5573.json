{"ast":null,"code":"export default {\n  data() {\n    return {\n      nameMessage: '',\n      idMessage: '',\n      emailMessage: '',\n      searchOption: '',\n      name: '',\n      optionName: \"이름\",\n      id: '',\n      email: ''\n    };\n  },\n\n  methods: {\n    chageSelect(event) {\n      if (event.target.value == \"id\") {\n        this.optionName = \"이름\";\n      } else {\n        this.optionName = \"아이디\";\n      }\n    },\n\n    submit() {\n      if (this.id == '' || this.email == '') {\n        alert(\"누락된 입력이 있습니다.\");\n      } else alert(\"확인\");\n    },\n\n    async searchInformation() {\n      // 로그인 함수 \n      return await axios({\n        //url:  \"https://95efaac5-e1c6-43d1-b04a-e97f801c4e7e.mock.pstmn.io/test\", //가상서버 주소\n        method: \"post\",\n        // post 명령어 \n        data: {\n          \"id\": this.loginid,\n          \"pw\": this.loginpw\n        }\n      }).then(res => {\n        // 데이터 준 다음 받은 응답\n        console.log(res.status);\n        alert(\"로그인에 성공했습니다.\");\n\n        if (res.status == 200) {\n          this.$router.push({\n            name: 'loginafter'\n          });\n        }\n      }).catch(e => {\n        alert(\"로그인에 실패했습니다. 계정 정보를 확인해주세요.\");\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,eAAe;EACPA,IAAI,GAAE;IACN,OAAM;MACFC,WAAU,EAAI,EADZ;MAEFC,SAAQ,EAAI,EAFV;MAGFC,YAAW,EAAI,EAHb;MAIFC,YAAW,EAAI,EAJb;MAKFC,IAAG,EAAI,EALL;MAMFC,UAAU,EAAC,IANT;MAOFC,EAAE,EAAC,EAPD;MAQFC,KAAK,EAAC;IARJ,CAAN;EAUC,CAZM;;EAaPC,OAAO,EAAC;IACRC,WAAW,CAACC,KAAD,EAAO;MACd,IAAGA,KAAK,CAACC,MAAN,CAAaC,KAAb,IAAsB,IAAzB,EAA8B;QAC1B,KAAKP,UAAL,GAAkB,IAAlB;MACJ,CAFA,MAGI;QACA,KAAKA,UAAL,GAAkB,KAAlB;MACJ;IACH,CARO;;IASRQ,MAAM,GAAE;MACJ,IAAG,KAAKP,EAAL,IAAW,EAAX,IAAiB,KAAKC,KAAL,IAAc,EAAlC,EACA;QACIO,KAAK,CAAC,eAAD,CAAL;MACJ,CAHA,MAIKA,KAAK,CAAC,IAAD,CAAL;IACR,CAfO;;IAgBR,MAAMC,iBAAN,GAA0B;MAAE;MAC5B,OACA,MAAMC,KAAK,CAAC;QACV;QACAC,MAAM,EAAE,MAFE;QAEM;QAChBlB,IAAI,EAAE;UAAC,MAAO,KAAKmB,OAAb;UAAsB,MAAM,KAAKC;QAAjC;MAHI,CAAD,CAAL,CA